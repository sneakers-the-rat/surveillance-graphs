@article{sadilekPrivacyfirstHealthResearch2021,
  title = {Privacy-First Health Research with Federated Learning},
  author = {Sadilek, Adam and Liu, Luyang and Nguyen, Dung and Kamruzzaman, Methun and Serghiou, Stylianos and Rader, Benjamin and Ingerman, Alex and Mellem, Stefan and Kairouz, Peter and Nsoesie, Elaine O. and MacFarlane, Jamie and Vullikanti, Anil and Marathe, Madhav and Eastham, Paul and Brownstein, John S. and y Arcas, Blaise Aguera and Howell, Michael D. and Hernandez, John},
  year = {2021},
  month = sep,
  journal = {npj Digital Medicine},
  volume = {4},
  number = {1},
  pages = {1--8},
  publisher = {{Nature Publishing Group}},
  issn = {2398-6352},
  doi = {10.1038/s41746-021-00489-2},
  url = {https://www.nature.com/articles/s41746-021-00489-2},
  urldate = {2023-03-29},
  abstract = {Privacy protection is paramount in conducting health research. However, studies often rely on data stored in a centralized repository, where analysis is done with full access to the sensitive underlying content. Recent advances in federated learning enable building complex machine-learned models that are trained in a distributed fashion. These techniques facilitate the calculation of research study endpoints such that private data never leaves a given device or healthcare system. We show\textemdash on a diverse set of single and multi-site health studies\textemdash that federated models can achieve similar accuracy, precision, and generalizability, and lead to the same interpretation as standard centralized statistical models while achieving considerably stronger privacy protections and without significantly raising computational costs. This work is the first to apply modern and general federated learning methods that explicitly incorporate differential privacy to clinical and epidemiological research\textemdash across a spectrum of units of federation, model architectures, complexity of learning tasks and diseases. As a result, it enables health research participants to remain in control of their data and still contribute to advancing science\textemdash aspects that used to be at odds with each other.},
  archive = {https://web.archive.org/web/20230329002813/https://www.nature.com/articles/s41746-021-00489-2},
  copyright = {2021 The Author(s)},
  langid = {english},
  keywords = {archived,clinical,google,Mathematics and computing,Medical research},
  file = {/Users/jonny/Dropbox/papers/zotero/S/SadilekA/sadilek_2021_privacy-first_health_research_with_federated_learning.pdf}
}

