  langid = {english},
  keywords = {archived,Computer Science - Artificial Intelligence,Computer Science - Computation and Language,knowledge graphs,microsoft},
  file = {/Users/jonny/Dropbox/papers/zotero/H/HuZ/hu_2022_empowering_language_models_with_knowledge_graph_reasoning_for_question_answering.pdf}
