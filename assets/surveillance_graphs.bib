  shorttitle = {From {{Platform}} to {{Knowledge Graph}}},
  author = {Bai, Jiaru and Cao, Liwei and Mosbach, Sebastian and Akroyd, Jethro and Lapkin, Alexei A. and Kraft, Markus},
  year = {2022},
  month = feb,
  journal = {JACS Au},
