@misc{metzlerRethinkingSearchMaking2021,
  title = {Rethinking {{Search}}: {{Making Domain Experts}} out of {{Dilettantes}}},
  shorttitle = {Rethinking {{Search}}},
  author = {Metzler, Donald and Tay, Yi and Bahri, Dara and Najork, Marc},
  year = {2021},
