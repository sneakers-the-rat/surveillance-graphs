  author = {{Province of Utrecht, the Netherlands} and Peters, Rob and Smit, Koen and {HU University of Applied Sciences Utrecht, Digital Ethics, the Netherlands} and Versendaal, Johan and {HU University of Applied Sciences Utrecht, Digital Ethics, the Netherlands}},
  year = {2021},
  journal = {34th Bled eConference Digital Support from Crisis to Progressive Change: Conference Proceedings},
