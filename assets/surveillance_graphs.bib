@article{swansonHowInformationSystems2020,
  title = {How Information Systems Came to Rule the World: {{Reflections}} on the Information Systems Field},
  shorttitle = {How Information Systems Came to Rule the World},
  author = {Swanson, E. Burton},
  year = {2020},
  month = mar,
  journal = {The Information Society},
  volume = {36},
