@article{dasilvanetoPlatformizationScienceScientific2023,
  title = {The {{Platformization}} of {{Science}}: {{Towards}} a {{Scientific Digital Platform Taxonomy}}},
  shorttitle = {The {{Platformization}} of {{Science}}},
  author = {{da Silva Neto}, Victo Jos{\'e} and Chiarini, Tulio},
  year = {2023},
  month = mar,
  journal = {Minerva},
  volume = {61},
